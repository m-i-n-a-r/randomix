    <!-- Roulette -->
    <string name="hello_roulette_fragment">Mou de ruleta</string>
    <string name="description_roulette">¿Tienes d\'escoyer daqué? ¿Fartesti d\'encuestes y d\'amistaes mandones? ¡La ruleta decide por ti! Tenla primida p\'amestar o quitar opciones xenériques.</string>
    <string name="spin_roulette">¡Xira la ruleta!</string>
    <string name="insert_roulette_hint">Introduz una opción…</string>
    <string name="delete_roulette_hint">Desanicia la última opción introducida</string>
    <string name="insert_roulette_description">Escribi abaxo toles posibilidaes p\'amestales a la ruleta.</string>
    <string name="no_entry_roulette">¡Introduz 2 o más opciones pa xirar la ruleta!</string>
    <string name="too_much_entries_roulette">¡Nun pues introducir más de 30 opciones!</string>
    <string name="generic_option">Opción</string>
    <string name="recent_title">Opciones de recién, toca pa restaurales</string>
    <string name="bottom_sheet_no_option">Nun hai nenguna opción recién. ¡Xira la ruleta pa guardar les opciones actuales!</string>
    <string name="letters_shortcut">De l\'A a la Z</string>
    <string name="range">Intervalu</string>
    <string name="wrong_range_roulette">L\'intervalu ye incorreutu o ta baleru. ¡Introduz correutamente dambos valores!</string>
    <string name="delete_all_roulette_confirmation">¿De xuru que quies desaniciar toles opciones introducíes?</string>

    <!-- Coin -->
    <string name="hello_coin_fragment">Mou de moneda</string>
    <string name="description_coin">Tirar al aire una moneda ye fácil, mas pue ayudate a decidir ente facer daqué o non. Cara o cruz...¡Dexa que la moneda lo diga!</string>
    <string name="flip_coin">¡Tira la moneda!</string>
    <string name="hint_coin">¡Primi\'l botón d\'abaxo!</string>
    <string name="result_head">¡Cara!</string>
    <string name="result_tail">¡Cruz!</string>

    <!-- Magic Ball -->
    <string name="hello_magic_ball_fragment">Bola máxica</string>
    <string name="description_magic_ball">¡La bola máxica respuéndelo too! Entruga daqué y dexa que t\'ayude con una rempuesta xenérica y afayadiza. ¡Nos axustes pue facer que seya tosca!</string>
    <string name="shake_ball">¡Ximiélgala!</string>
    <string name="hint_magic_ball">¡Primi\'l botón d\'abaxo!</string>
    <string name="magic_answer_1">Non.</string>
    <string name="magic_answer_2">¡Sí!</string>
    <string name="magic_answer_3">Nun me paez.</string>
    <string name="magic_answer_4">¡Seique!</string>
    <string name="magic_answer_5">Nun tien sentíu.</string>
    <string name="magic_answer_6">Probablemente…</string>
    <string name="magic_answer_7">¿De xuru?</string>
    <string name="magic_answer_8">Igual sí.</string>
    <string name="magic_answer_9">Igual non.</string>
    <string name="magic_answer_10">Ta claro.</string>
    <string name="magic_answer_11">Faite la entruga.</string>
    <string name="magic_answer_12">Non, ye una mazcayada.</string>
    <string name="magic_answer_13">¡Sí, señor!</string>
    <string name="magic_answer_14">Piensa nello.</string>
    <string name="magic_answer_15">Quién sabe.</string>
    <string name="magic_answer_16">Nun fastidies.</string>
    <string name="magic_answer_17">Polo que veo, sí.</string>
    <string name="magic_answer_18">Ye cierto.</string>
    <string name="magic_answer_19">Ye incuestionable.</string>
    <string name="magic_answer_20">Ye un bon pronósticu.</string>
    <string name="magic_answer_21">Too indica que sí.</string>
    <string name="magic_answer_22">Ensin nenguna dulda.</string>
    <string name="magic_answer_23">Ye confuso, volvi entrugar.</string>
    <string name="magic_answer_24">Meyor nun lo digo agora.</string>
    <string name="magic_answer_25">¡Nun soi a predicilo!</string>
    <string name="magic_answer_26">Concéntrate y volvi entrugar.</string>
    <string name="magic_answer_27">Nun cuentes con ello.</string>
    <string name="magic_answer_28">Les mios fontes dicen que non.</string>
    <string name="magic_answer_29">Ye mui duldoso.</string>
    <string name="magic_answer_30">Entrúgalo a les amistaes.</string>
    <string name="magic_answer_rude_1">Non, idiota.</string>
    <string name="magic_answer_rude_2">Ta claro, hostia.</string>
    <string name="magic_answer_rude_3">¿Chupes el deu?</string>
    <string name="magic_answer_rude_4">¡Ḥaḥa! Vaya fatada.</string>
    <string name="magic_answer_rude_5">Tas ensin cepiyar.</string>
    <string name="magic_answer_rude_6">Vete al infiernu.</string>

    <!-- Dice -->
    <string name="hello_dice_fragment">Mou de dau</string>
    <string name="description_dice">El dau pue ser útil, por exemplu, nun xuegu de mesa. L\'aplicación va tirar el númberu de daos qu\'esbilles nos axustes (de 1 a 10). ¡Bona suerte!</string>
    <string name="throw_dice">¡Tira\'l dau!</string>
    <string name="hint_dice">¡Primi\'l botón d\'abaxo!</string>

