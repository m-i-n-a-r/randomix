    <!-- Roulette -->
    <string name="hello_roulette_fragment">Roulette Modus</string>
    <string name="description_roulette">Musst du dich für etwas entscheiden? Bist du müde von Umfragen und rechthaberischen Freunden? Ein
        Roulette wird für dich entscheiden! Drücke lange auf das Roulette, um generische Optionen hinzuzufügen oder sie zu löschen.</string>
    <string name="spin_roulette">Drehe das Roulette!</string>
    <string name="insert_roulette_hint">Geben Sie eine Option ein…</string>
    <string name="delete_roulette_hint">Lösche die zuletzt eingegebene Option</string>
    <string name="insert_roulette_description">Bitte liste unten die Optionen auf, die zum Roulette hinzugefügt werden sollen</string>
    <string name="no_entry_roulette">Bitte, gib zwei oder mehr Optionen ein um das Roulette zu drehen!</string>
    <string name="too_much_entries_roulette">Du kannst nicht mehr als 15 Optionen eingeben!</string>
    <string name="generic_option">Option</string>
    <string name="recent_title">Letzte Optionen, tippe zum Wiederherstellen</string>
    <string name="bottom_sheet_no_option">Keine letzten Optionen, drehe das Roulette um die aktuellen Optionen zu speichern!</string>
    <string name="letters_shortcut">Buchstaben von A-Z</string>

    <!-- Coin -->
    <string name="hello_coin_fragment">Münz Modus</string>
    <string name="description_coin">Eine Münze zu werfen ist einfach, kann dir aber dabei helfen, dich zu entscheiden, ob du etwas tust oder nicht.         Einfach Kopf oder Zahl. Lass die Münze sprechen!</string>
    <string name="flip_coin">Wirf die Münze!</string>
    <string name="hint_coin">Drücke den Knopf unten!</string>
    <string name="result_head">Kopf!</string>
    <string name="result_tail">Zahl!</string>

    <!-- Magic Ball -->
    <string name="hello_magic_ball_fragment">Magische Kugel</string>
    <string name="description_magic_ball">Die magische Kugel wird dir die Antworten sagen! Frag einfach etwas, und
        er wird dir mit einer generischen und günstigen Antwort helfen. Du kannst unhöfliche Antworten in den Einstellungen aktivieren!</string>
    <string name="shake_ball">Schüttle es!</string>
    <string name="hint_magic_ball">Drücke den Knopf unten!</string>
    <string name="magic_answer_1">Nein.</string>
    <string name="magic_answer_2">Ja!</string>
    <string name="magic_answer_3">Ich glaube nicht.</string>
    <string name="magic_answer_4">Vielleicht!</string>
    <string name="magic_answer_5">Das macht keinen Sinn.</string>
    <string name="magic_answer_6">Wahrscheinlich…</string>
    <string name="magic_answer_7">Bist du sicher?</string>
    <string name="magic_answer_8">Wahrscheinlich ja.</string>
    <string name="magic_answer_9">Wahrscheinlich nein.</string>
    <string name="magic_answer_10">Ja, natürlich.</string>
    <string name="magic_answer_11">Frag dich selbst.</string>
    <string name="magic_answer_12">Nein, das ist absurd.</string>
    <string name="magic_answer_13">Zur Hölle ja!</string>
    <string name="magic_answer_14">Denk mal darüber nach.</string>
    <string name="magic_answer_15">Wer weiß.</string>
    <string name="magic_answer_16">Machst du Witze?</string>
    <string name="magic_answer_17">So wie ich das sehe, ja.</string>
    <string name="magic_answer_18">Das steht fest.</string>
    <string name="magic_answer_19">Das ist eindeutig so.</string>
    <string name="magic_answer_20">Gute Aussichten.</string>
    <string name="magic_answer_21">Die Zeichen stehen auf Ja.</string>
    <string name="magic_answer_22">Zweifellos.</string>
    <string name="magic_answer_23">Antwort verschwommen, versuche es erneut.</string>
    <string name="magic_answer_24">Ich sage es dir jetzt besser nicht.</string>
    <string name="magic_answer_25">Kann ich jetzt nicht vorhersagen!</string>
    <string name="magic_answer_26">Konzentriere dich und frage erneut.</string>
    <string name="magic_answer_27">Verlasse dich nicht darauf.</string>
    <string name="magic_answer_28">Meine Quellen sagen nein.</string>
    <string name="magic_answer_29">Sehr zweifelhaft.</string>
    <string name="magic_answer_30">Frag deinen besten Freund.</string>
    <string name="magic_answer_rude_1">Nein, du Idiot.</string>
    <string name="magic_answer_rude_2">Natürlich, verdammt noch mal.</string>
    <string name="magic_answer_rude_3">Bist du bescheuert?</string>
    <string name="magic_answer_rude_4">Lol! Wie dumm.</string>
    <string name="magic_answer_rude_5">Du bist verrückt.</string>
    <string name="magic_answer_rude_6">Fahr zur Hölle.</string>

