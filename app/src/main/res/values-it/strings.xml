    <string name="wrong_range_roulette">L\'intervallo è sbagliato o vuoto, per favore inserisci entrambi i valori correttamente!</string>
    <string name="delete_all_roulette_confirmation">Sei sicuro di voler eliminare tutte le opzioni inserite?</string>

    <!-- Coin -->
    <string name="hello_coin_fragment">Modalità moneta</string>
    <string name="description_coin">Tirare una moneta è semplice, ma può aiutarti a decidere se fare qualcosa o meno. Semplicemente testa o croce. Fai parlare la moneta!</string>
    <string name="flip_coin">Lancia la moneta!</string>
    <string name="hint_coin">Premi il bottone sotto!</string>
    <string name="result_head">Testa!</string>
    <string name="result_tail">Croce!</string>

    <!-- Magic Ball -->
    <string name="hello_magic_ball_fragment">Palla Magica</string>
    <string name="description_magic_ball">La palla magica ti dirà la risposta! Chiedi semplicemente qualcosa, e ti aiuterà con una risposta generica e affidabile. Puoi attivare la maleducazione dalle impostazioni!</string>
    <string name="shake_ball">Scuotila!</string>
    <string name="hint_magic_ball">Premi il bottone sotto!</string>
    <string name="magic_answer_1">No.</string>
    <string name="magic_answer_2">Si!</string>
    <string name="magic_answer_3">Io non credo.</string>
    <string name="magic_answer_4">Forse!</string>
    <string name="magic_answer_5">Non ha senso.</string>
    <string name="magic_answer_6">Probabilmente…</string>
    <string name="magic_answer_7">Sei sicuro/a?</string>
    <string name="magic_answer_8">Probabilmente si.</string>
    <string name="magic_answer_9">Probabilmente no.</string>
    <string name="magic_answer_10">Ovviamente.</string>
    <string name="magic_answer_11">Chiedilo a te stesso/a.</string>
    <string name="magic_answer_12">No, assurdo.</string>
    <string name="magic_answer_13">Cavolo si!</string>
    <string name="magic_answer_14">Pensaci da solo.</string>
    <string name="magic_answer_15">E chi lo sa.</string>
    <string name="magic_answer_16">Ma stai scherzando?</string>
    <string name="magic_answer_17">Per quanto posso vedere, si.</string>
    <string name="magic_answer_18">È certo.</string>
    <string name="magic_answer_19">È decisamente così.</string>
    <string name="magic_answer_20">Le prospettive sono buone.</string>
    <string name="magic_answer_21">I segni indicano di si.</string>
    <string name="magic_answer_22">Senza alcun dubbio.</string>
    <string name="magic_answer_23">È difficile rispondere, prova di nuovo.</string>
    <string name="magic_answer_24">Meglio non risponderti adesso.</string>
    <string name="magic_answer_25">Non posso predirlo ora!</string>
    <string name="magic_answer_26">Concentrati e rifai la domanda.</string>
    <string name="magic_answer_27">Non ci contare.</string>
    <string name="magic_answer_28">Le mie fonti dicono di no.</string>
    <string name="magic_answer_29">Molto incerto.</string>
    <string name="magic_answer_30">Chiedi al tuo migliore amico.</string>
    <string name="magic_answer_rude_1">No, idiota.</string>
    <string name="magic_answer_rude_2">Ovvio, maledizione.</string>
    <string name="magic_answer_rude_3">Ma sei stupido/a?</string>
    <string name="magic_answer_rude_4">Lol! Che idiozia.</string>
    <string name="magic_answer_rude_5">Tu sei pazzo/a.</string>
    <string name="magic_answer_rude_6">Va all\'inferno.</string>

