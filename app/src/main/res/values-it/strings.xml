    <!-- Roulette -->
    <string name="hello_roulette_fragment">Modalità ruota</string>
    <string name="description_roulette">Devi scegliere qualcosa? Sei stanco di fare sondaggi e degli amici che vogliono comandare? Una ruota deciderà per te! Premi a lungo sulla ruota per aggiungere opzioni generiche o eliminare quelle inserite.</string>
    <string name="spin_roulette">Gira la ruota!</string>
    <string name="insert_roulette_hint">Inserisci un\'opzione…</string>
    <string name="delete_roulette_hint">Elimina l\'ultima opzione inserita</string>
    <string name="insert_roulette_description">Per favore inserisci qui sotto le possibili scelte da aggiungere nella ruota.</string>
    <string name="no_entry_roulette">Per favore, inserisci almeno due opzioni per girare la roulette!</string>
    <string name="too_much_entries_roulette">Non puoi inserire più di 30 opzioni!</string>
    <string name="generic_option">Opzione</string>
    <string name="recent_title">Opzioni recenti, tocca per ripristinare</string>
    <string name="bottom_sheet_no_option">Nessuna opzione recente, gira la roulette per salvare le opzioni correnti!</string>
    <string name="letters_shortcut">Lettere A-Z</string>
