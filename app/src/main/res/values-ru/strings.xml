    <!-- Coin -->
    <string name="hello_coin_fragment">Монетка</string>
    <string name="description_coin">Подбросить монетку легко, но она может помочь вам решить, когда что-то делать или нет. Только орёл или решка. Пусть говорит монета!</string>
    <string name="flip_coin">Подбросить монетку!</string>
    <string name="hint_coin">Нажмите кнопку ниже!</string>
