    <string name="hint_coin">点击下方的按钮！</string>
    <string name="result_head">正面！</string>
    <string name="result_tail">反面！</string>

    <!-- Magic Ball -->
    <string name="hello_magic_ball_fragment">魔力球</string>
    <string name="description_magic_ball">魔力球会告诉你答案！只需要问些什么，它就会告诉你一个通用又可以负担的答案。你还可以通过设置允许它变得粗鲁！</string>
    <string name="shake_ball">摇动它！</string>
    <string name="hint_magic_ball">点击下方的按钮！</string>
    <string name="magic_answer_1">不是。</string>
    <string name="magic_answer_2">是的！</string>
    <string name="magic_answer_3">我不这么认为。</string>
    <string name="magic_answer_4">可能吧！</string>
    <string name="magic_answer_5">这没道理。</string>
    <string name="magic_answer_6">大概吧……</string>
    <string name="magic_answer_7">你确定吗？</string>
    <string name="magic_answer_8">大概是的。</string>
    <string name="magic_answer_9">大概不是。</string>
    <string name="magic_answer_10">当然。</string>
    <string name="magic_answer_11">问你自己。</string>
    <string name="magic_answer_12">不，荒谬。</string>
    <string name="magic_answer_13">当然好啦！</string>
    <string name="magic_answer_14">想想看。</string>
    <string name="magic_answer_15">谁知道呢。</string>
    <string name="magic_answer_16">你在开玩笑吗？</string>
    <string name="magic_answer_17">就我看来，是的。</string>
    <string name="magic_answer_18">这很确定。</string>
    <string name="magic_answer_19">肯定是这样。</string>
    <string name="magic_answer_20">前景不错。</string>
    <string name="magic_answer_21">迹象显示，是。</string>
    <string name="magic_answer_22">无需犹豫。</string>
    <string name="magic_answer_23">回应模糊不清，再试一次吧。</string>
    <string name="magic_answer_24">现在最好别告诉你。</string>
    <string name="magic_answer_25">现在无法预测！</string>
    <string name="magic_answer_26">集中精神再问一遍。</string>
    <string name="magic_answer_27">别指望它。</string>
    <string name="magic_answer_28">我的信息来源指出，不是。</string>
    <string name="magic_answer_29">非常可疑。</string>
    <string name="magic_answer_30">去问你最好的朋友吧。</string>
    <string name="magic_answer_rude_1">不，傻逼。</string>
    <string name="magic_answer_rude_2">当然啦，混蛋。</string>
    <string name="magic_answer_rude_3">你是傻子吗？</string>
    <string name="magic_answer_rude_4">哈！真蠢。</string>
    <string name="magic_answer_rude_5">你他妈疯了吧。</string>
    <string name="magic_answer_rude_6">去死吧。</string>

